{
  "name": "started-from-a-golf-pool",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "jwks-rsa": "^1.4.0",
    "mongoose": "^5.4.17",
    "node-schedule": "^1.3.1",
    "nodemailer": "^5.1.1",
    "redis": "^2.8.0",
    "redis-rejson": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
